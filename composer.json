{
  "name": "rad-laravel/components",
  "description": "Laravel Component Management",
  "keywords": [
    "laravel",
    "rad",
    "component"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "anonymoussc",
      "email": "50c5ac69@opayq.com",
      "role": "Dev"
    }
  ],
  "require": {
    "php": ">=5.5",
    "laravel/framework": "5.1.*|5.2.*|5.3.*",
    "laravelcollective/html": "5.1.*|5.2.*|5.3.*"
  },
  "require-dev": {
    "phpunit/phpunit": "~4",
    "mockery/mockery": "~0.9",
    "orchestra/testbench": "^3.1|^3.2|^3.3",
    "phpro/grumphp": "^0.9.1",
    "friendsofphp/php-cs-fixer": "^1.11"
  },
  "autoload": {
    "psr-4": {
      "Rad\\Components\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Rad\\Components\\Tests\\": "tests"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
